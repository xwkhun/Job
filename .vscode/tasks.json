{
    "tasks": [
        {
            "type": "shell",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/ID:/Program Files (x86)/Microsoft Visual Studio/2019/Professional/VC/Tools/MSVC/14.29.30133/include",
                "/ID:/Windows Kits/10/Include/10.0.19041.0/shared",
                "/ID:/Windows Kits/10/Include/10.0.19041.0/ucrt",
                "/ID:/Windows Kits/10/Include/10.0.19041.0/um",
                "/ID:/Windows Kits/10/Include/10.0.19041.0/winrt",
                "/nologo",
                "/Fe:",
                "${fileDirname}/${fileBasenameNoExtension}.exe",
                "${file}",
                "/link",
                "/LIBPATH: D:/Program Files (x86)/Microsoft Visual Studio/2019/Professional/VC/Tools/MSVC/14.29.30133/lib/x64",
                "/LIBPATH: D:/Windows Kits/10/Lib/10.0.19041.0/ucrt/x64",
                "/LIBPATH: D:/Windows Kits/10/Lib/10.0.19041.0/um/x64"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "compiler: cl.exe"
        }
    ],
    "version": "2.0.0"
}